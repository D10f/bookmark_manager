{
    "compilerOptions": {
        "target": "ES2022",
        "module": "commonjs",
        "baseUrl": "./",
        "paths": {
            "@/*": ["resources/js/*"]
        },
        "isolatedModules": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
